#################################################################

###  http://data.ashrae.org/standard223#contains
s223:contains rdf:type owl:AnnotationProperty ;
              s223:inverseOf s223:isContainedIn ;
              rdfs:label "contains" .


###  http://data.ashrae.org/standard223#hasActuatorPosition
s223:hasActuatorPosition rdfs:label "Actuator Position" ;
                         rdf:type owl:AnnotationProperty ;
                         rdfs:subPropertyOf s223:hasProperty .


###  http://data.ashrae.org/standard223#hasDevice
s223:hasDevice rdf:type owl:AnnotationProperty ;
               rdfs:subPropertyOf s223:contains .


###  http://data.ashrae.org/standard223#hasInletPressure
s223:hasInletPressure rdfs:label "Inlet Pressure" ;
                      rdf:type owl:AnnotationProperty ;
                      rdfs:subPropertyOf s223:hasProperty .


###  http://data.ashrae.org/standard223#hasJunction
s223:hasJunction rdfs:label "has junction" ;
                 rdf:type owl:AnnotationProperty ;
                 rdfs:subPropertyOf s223:contains .


###  http://data.ashrae.org/standard223#hasMaxActuatorTorque
s223:hasMaxActuatorTorque rdfs:label "Maximum torque for the actuator" ;
                          rdf:type owl:AnnotationProperty ;
                          rdfs:subPropertyOf s223:hasProperty .


###  http://data.ashrae.org/standard223#hasOutletPressure
s223:hasOutletPressure rdfs:label "Outlet Pressure" ;
                       rdf:type owl:AnnotationProperty ;
                       rdfs:subPropertyOf s223:hasProperty .


###  http://data.ashrae.org/standard223#hasProperty
