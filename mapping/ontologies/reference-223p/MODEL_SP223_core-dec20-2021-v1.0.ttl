               rdfs:label "inverse of" .


###  http://data.ashrae.org/standard223#isContainedIn
s223:isContainedIn rdf:type owl:AnnotationProperty ;
                   s223:inverseOf s223:contains ;
                   rdfs:label "is contained in" .


###  http://data.ashrae.org/standard223#isDeviceOf
s223:isDeviceOf s223:inverseOf s223:hasDevice ;
                rdfs:label "is device of" ;
                rdf:type owl:AnnotationProperty ;
                rdfs:subPropertyOf s223:isContainedIn ;
                rdfs:domain s223:Device .


###  http://data.ashrae.org/standard223#isSubSystemOf
s223:isSubSystemOf rdf:type owl:AnnotationProperty ;
                   rdfs:subPropertyOf s223:isContainedIn .


