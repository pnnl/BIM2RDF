  rdfs:subClassOf s223:Device ;
.
s223:VirtualSensor
  a s223:Class ;
  a sh:NodeShape ;
  rdfs:label "Virtual sensor" ;
  rdfs:subClassOf s223:Sensor ;
  sh:property [
      sh:path s223:hasFunctionInput ;
      sh:class s223:Property ;
    ] ;
  sh:property [
      sh:path s223:hasMeasurementLocation ;
      sh:maxCount 0 ;
    ] ;
.
