PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX s223: <http://data.ashrae.org/standard223#>
PREFIX spkl: <http://speckle.systems/> 
prefix xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX meta: <http://meta>

CONSTRUCT { 
  ?this s223:cnx  _:cp.
  ?this s223:hasConnectionPoint  _:cp.
  _:cp s223:isConnectionPointOf ?this.
  _:cp  a s223:OutletConnectionPoint.
  _:cp rdfs:label "OutletConnectionPoint"^^xsd:string.
  _:cp s223:hasMedium s223:Light-Visible.
} 
WHERE { 
    ?this a s223:Luminaire.
}
